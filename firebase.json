{
  "hosting": {
    "public": "out",
    "redirects": [
      {
        "source" : "/archives/index.rss",
        "destination" : "/static/feeds/archives.xml",
        "type" : 301
      },
      {
        "source" : "/memo/index.rss",
        "destination" : "/static/feeds/blog.xml",
        "type" : 301
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
